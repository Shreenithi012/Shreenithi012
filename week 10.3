list1 = [int(x) for x in input().split()]

list2 = sorted(list(set(list1)))
freq=[]
for i in list2:
    len1 = list1.count(i)
    if len1>=2:
        for j in range(len1):
            list1.remove(i)
    
    freq.append(len1)
    
i=0
while i<len(list2):
    print("{} {}".format(list2[i],freq[i]))
    i=i+1
